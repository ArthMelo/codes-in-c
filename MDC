#include<stdio.h>

int mdc(int m, int n)
{
    if (n == 0)
    {
        return m;
    }
    return mdc(n, m%n);
}

int main(void)
{
    int x, y, resposta;
    printf("Digite dois numeros, sendo o primeiro deles o maior\n");
    scanf("%d %d", &x, &y);

    if (x < y)
    {
        printf("Numeros invalidos\n");
        return 0;
    }

    resposta = mdc(x, y);

    printf("%d", resposta);
    return 0;

}
