#include<stdio.h>

int mdc(int m, int n)
{
    if (n == 0)
    {
        return m;
    }
    return mdc(n, m%n);
}

int main(void)
{
    int x, y, resposta;
    printf("Digite dois numeros\n");
    scanf("%d %d", &x, &y);

    if (x < y)
    {
        resposta = mdc(y, x);
        printf("O MDC eh %d", resposta);
        return 0;
    }

    resposta = mdc(x, y);

    printf("O MDC eh %d", resposta);
    return 0;

}
